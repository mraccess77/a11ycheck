{
  "manifest_version": 2,

  "name": "A11y checker",
  "description": "Accessibility checker",
  "version": "1.0",
  "browser_action": {
  },
  "background": {
  	"scripts": ["background.js"],
  	"persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["a11y.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}
